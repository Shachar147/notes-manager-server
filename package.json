{
  "name": "notes-manager-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "start:dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "worker:audit": "ts-node src/workers/audit.worker.ts",
    "worker:audit:dev": "ts-node-dev --respawn --transpile-only src/workers/audit.worker.ts",
    "worker:notes": "ts-node src/workers/notes.worker.ts",
    "worker:notes:dev": "ts-node-dev --respawn --transpile-only src/workers/notes.worker.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d src/config/database.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/config/database.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/config/database.ts",
    "test": "jest --maxWorkers=8",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^9.0.2",
    "@types/amqplib": "^0.10.4",
    "@types/express": "^4.17.21",
    "amqplib": "^0.10.3",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "google-auth-library": "^10.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^7.1.1",
    "redis-cli": "^2.1.2",
    "redlock": "^5.0.0-beta.2",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.0",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
